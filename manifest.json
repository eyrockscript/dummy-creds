{
  "manifest_version": 3,
  "name": "Dummy Credentials",
  "version": "1.0.0",
  "description": "Generate dummy usernames, emails, and strong passwords for testing purposes",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Dummy Credentials Generator"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}